{
  "name": "willo-wiki-firebase",
  "version": "1.1.0",
  "description": "Firebase-specific elements for the Willo Wiki project",
  "scripts": {
    "-= DEV =-": "",
    "setup:dev": "npm --prefix ./functions/ i",
    "-= CI =-": "",
    "setup:ci": "npm i && npm --prefix ./functions/ i",
    "build:ci": "firebase deploy -P $FIREBASE_CI_ENV --token $FIREBASE_CI_TOKEN"
  },
  "dependencies": {
    "firebase-tools": "^9.20.0"
  }
}
